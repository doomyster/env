[org/mate/marco/global-keybindings]
switch-to-workspace-right='<Mod4>Right'
switch-to-workspace-left='<Mod4>Left'
switch-to-workspace-down='<Mod4>Down'
switch-to-workspace-up='<Mod4>Up'

[org/mate/marco/window-keybindings]
move-to-workspace-right='<Shift><Mod4>Right'
move-to-workspace-left='<Shift><Mod4>Left'
move-to-workspace-down='<Shift><Mod4>Down'
move-to-workspace-up='<Shift><Mod4>Up'

[org/mate/panel/objects/workspace-switcher/prefs]
display-all-workspaces=true
num-rows=4

[org/gnome/desktop/wm/preferences]
num-workspaces=32



